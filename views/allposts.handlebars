{{#each posts as |post|}}
  <div> class="card col-12"
      <div class="card-body">
          <h3 class="card-title post-card-title">{{post.title}}</h3>
          <p class="card-text">{{post.content}}</p>p>
          <h5 class="card-subtitle mb-2 text-muted">{{post.user.username}} on {{post.postedAt}}</h5>
      </div>
  </div>
{{/each}}